{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "linebreak-style": ["off", "windows"],
    "indent": ["off", "tab"],
    "react/jsx-filename-extension": "off"
  }
}
